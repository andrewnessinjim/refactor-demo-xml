<?xml version="1.0" encoding="UTF-8"?>
<petSearchRequest xmlns:f="http://www.w3.org/2005/xpath-functions">
    <metadata>
        <requestTimestamp>#timestamp#</requestTimestamp>
        <correlationId>#requestId#</correlationId>
    </metadata>
    <data>
        <searchCriteria>
            <criterion>
                <key>pet</key>
                <value>#pet#</value>
            </criterion>
            <criterion>
                <key>parrotColor</key>
                <value>#parrotColor#</value>
            </criterion>
            <criterion>
                <key>canParrotTalk</key>
                <value>#canParrotTalk#</value>
            </criterion>
            <criterion>
                <key>parrotNeedsCage</key>
                <value>#parrotNeedsCage#</value>
            </criterion>
        </searchCriteria>
    </data>
</petSearchRequest>