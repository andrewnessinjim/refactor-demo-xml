<?xml version="1.0" encoding="UTF-8"?>
<petSearchRequest xmlns:f="http://www.w3.org/2005/xpath-functions">
    <metadata>
        <requestTimestamp>#timestamp#</requestTimestamp>
        <correlationId>#requestId#</correlationId>
    </metadata>
    <data>
        <searchCriteria>
            <criterion>
                <key>pet</key>
                <value>#pet#</value>
            </criterion>
            <criterion>
                <key>dogSize</key>
                <value>#dogSize#</value>
            </criterion>
            <criterion>
                <key>isDogTrained</key>
                <value>#isDogTrained#</value>
            </criterion>
            <criterion>
                <key>dogAge</key>
                <value>#dogAge#</value>
            </criterion>
        </searchCriteria>
    </data>
</petSearchRequest>